<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="cn.wegfan.forum.mapper.TopicOperationLogMapper">
    <select id="selectListByTopicId" resultType="TopicOperationLog">
        SELECT *
        FROM topic_operation_log
        WHERE topic_id = #{topicId}
    </select>
</mapper>